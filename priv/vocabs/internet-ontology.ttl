@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@prefix bfo: <http://purl.obolibrary.org/bfo/bfo.owl#> .
@prefix schema: <https://schema.org/> .
@prefix as: <http://www.w3.org/ns/activitystreams#> .

@prefix net: <https://node.town/net/> .
@prefix code: <https://node.town/code/> .
@prefix ai: <https://node.town/ai/> .

<https://node.town/net/> a owl:Ontology ;
  rdfs:comment "Node.Town Internet Ontology 1.0"@en .

### AI stuff

ai:OpenAI a as:Organization ;
  as:name "OpenAI" .

ai:GPT3 a as:Service ;
  as:name "OpenAI GPT-3" ;
  as:partOf ai:OpenAI .

ai:Codex a as:Service ;
  as:name "OpenAI Codex" ;
  as:partOf ai:OpenAI .

ai:Model a as:Object ;
  as:name "Model"@en .

ai:GPT3TextDavinci3 a ai:Model ;
  as:name "text-davinci-003" ;
  as:partOf ai:GPT3 .

ai:GPT3TextCurie1 a ai:Model ;
  as:name "text-curie-001" ;
  as:partOf ai:GPT3 .

ai:GPT3TextBabbage1 a ai:Model ;
  as:name "text-babbage-001" ;
  as:partOf ai:GPT3 .

ai:GPT3TextAda1 a ai:Model ;
  as:name "text-ada-001" ;
  as:partOf ai:GPT3 .

ai:GPT3CodeDavinci2 a ai:Model ;
  as:name "code-davinci-002" ;
  as:partOf ai:Codex .

ai:GPT3CodeCushman2 a ai:Model ;
  as:name "code-cushman-002" ;
  as:partOf ai:Codex .

ai:Inference a owl:Class ;
  rdfs:subClassOf as:Object .

ai:TextCompletion a owl:Class ;
  rdfs:subClassOf ai:Inference .

ai:Embedding a owl:Class ;
  rdfs:subClassOf ai:Inference .

ai:input a owl:FunctionalProperty, owl:DatatypeProperty ;
  rdfs:domain ai:Inference ;
  rdfs:range xsd:string .

ai:output a owl:FunctionalProperty, owl:DatatypeProperty ;
  rdfs:domain ai:Inference ;
  rdfs:range xsd:string .

ai:model a owl:FunctionalProperty, owl:ObjectProperty ;
  rdfs:domain ai:Inference ;
  rdfs:range ai:Model .

### Conversation agents

ai:chatContext a owl:DatatypeProperty ;
  rdfs:domain as:Note ;
  rdfs:range xsd:string .

ai:NodeTown a as:Service ;
  as:name "Node.Town" ;
  as:url "https://node.town" .

ai:Classify a as:Service .

ai:Weather a as:Service .

ai:EInk a as:Service .

ai:Scrape a as:Service .

### Messenger stuff

net:originPlatform a owl:ObjectProperty ;
  rdfs:domain as:Object ;
  rdfs:range as:Object .

net:Telegram a as:Service ;
  as:name "Telegram"@en ;
  as:url "https://telegram.org" .

net:telegramId a owl:DatatypeProperty ;
  rdfs:domain as:Object ;
  rdfs:range xsd:integer .

net:telegramData a owl:DatatypeProperty ;
  rdfs:domain as:Object ;
  rdfs:range xsd:string .

net:jsonPayload a owl:DatatypeProperty ;
  rdfs:domain as:Update ;
  rdfs:range xsd:string .

net:Readwise a as:Service .

net:highlights a owl:ObjectProperty ;
  rdfs:domain schema:CreativeWork ;
  rdfs:range schema:Quotation .

code:CodeUnit a as:Object .
code:Module a code:CodeUnit .
code:Function a code:CodeUnit .

code:arity a owl:DatatypeProperty ;
  rdfs:domain code:Function ;
  rdfs:range xsd:integer .

code:documentation a owl:DatatypeProperty ;
  rdfs:domain code:CodeUnit ;
  rdfs:range xsd:string .

code:parent a owl:ObjectProperty ;
  rdfs:domain code:CodeUnit ;
  rdfs:range code:CodeUnit .

code:child a owl:ObjectProperty ;
  rdfs:domain code:CodeUnit ;
  rdfs:range code:CodeUnit .
