@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@prefix bfo: <http://purl.obolibrary.org/bfo/bfo.owl#> .
@prefix schema: <https://schema.org/> .
@prefix as: <http://www.w3.org/ns/activitystreams#> .

@prefix nt: <https://node.town/> .
@prefix code: <https://node.town/code/> .

<https://node.town/> a owl:Ontology ;
  rdfs:comment "Node.Town Internet Ontology 1.0"@en .

nt:API a owl:Class ; rdfs:label "API" ; rdfs:subClassOf as:Service .
nt:GenerativeModel a owl:Class ; as:name "Generative Model"@en .

nt:openai a as:Organization ; as:name "OpenAI" .
nt:gpt3   a nt:API          ; as:name "GPT-3 API" ;
  as:attributedTo nt:openai ;
        as:summary "GPT-3 is a language model developed by OpenAI. It is the successor to GPT-2, and is the third in the GPT series of language models. GPT-3 is a large transformer-based language model with 175 billion parameters, trained on a dataset of 45 GB of text. It is capable of generating coherent paragraphs of text on a wide range of topics, and can perform a variety of language tasks such as translation, summarization, and question answering. It is also capable of performing rudimentary mathematical calculations and solving simple puzzles."@en ;

nt:text_davinci_003  a nt:GenerativeModel .
nt:text_curie_001    a nt:GenerativeModel .
nt:text_babbage_001  a nt:GenerativeModel .
nt:code_davinci_002  a nt:GenerativeModel .
nt:code_cushman_002  a nt:GenerativeModel .

nt:Inference a owl:Class ;
  rdfs:subClassOf as:Object .

nt:TextCompletion a owl:Class ;
  rdfs:subClassOf nt:Inference .

nt:Embedding a owl:Class ;
  rdfs:subClassOf nt:Inference .

nt:input a owl:FunctionalProperty, owl:DatatypeProperty ;
  rdfs:domain nt:Inference ;
  rdfs:range xsd:string .

nt:output a owl:FunctionalProperty, owl:DatatypeProperty ;
  rdfs:domain nt:Inference ;
  rdfs:range xsd:string .

nt:model a owl:FunctionalProperty, owl:ObjectProperty ;
  rdfs:domain nt:Inference ;
  rdfs:range nt:Model .

### Conversation agents

nt:chatContext a owl:DatatypeProperty ;
  rdfs:domain as:Note ;
  rdfs:range xsd:string .

nt:NodeTown a as:Service ;
  as:name "Node.Town" ;
  as:url <https://node.town> .

nt:Classify a as:Service .
nt:Weather a as:Service .
nt:EInk a as:Service .
nt:Scrape a as:Service .

### Messenger stuff

nt:platform a owl:ObjectProperty ;
  rdfs:label "platform" ;
  rdfs:domain as:Object ;
  rdfs:range as:Object .

nt:Telegram a as:Service ;
  rdfs:label "Telegram" ;
  as:name "Telegram"@en ;
  as:url <https://telegram.org> .

nt:Discord a as:Service ;
  rdfs:label "Discord" ;
  as:name "Discord"@en ;
  as:url <https://discord.com> .

nt:idScope a owl:ObjectProperty ;
  rdfs:domain owl:Property ;
  rdfs:range as:Service .

nt:telegramId a owl:DatatypeProperty ;
  nt:idScope nt:Telegram ;
  rdfs:label "Telegram ID" ;
  rdfs:domain as:Object ;
  rdfs:range xsd:integer .

nt:discordId a owl:DatatypeProperty ;
  nt:idScope nt:Discord ;
  rdfs:label "Discord ID" ;
  rdfs:domain as:Object ;
  rdfs:range xsd:integer .

nt:jsonPayload a owl:DatatypeProperty ;
  rdfs:label "JSON Payload" ;
  rdfs:domain as:Update ;
  rdfs:range xsd:string .

nt:Readwise a as:Service .

nt:highlights a owl:ObjectProperty ;
  rdfs:domain schema:CreativeWork ;
  rdfs:range schema:Quotation .

code:CodeUnit a as:Object .
code:Module a code:CodeUnit .
code:Function a code:CodeUnit .

code:arity a owl:DatatypeProperty ;
  rdfs:domain code:Function ;
  rdfs:range xsd:integer .

code:documentation a owl:DatatypeProperty ;
  rdfs:domain code:CodeUnit ;
  rdfs:range xsd:string .

code:parent a owl:ObjectProperty ;
  rdfs:domain code:CodeUnit ;
  rdfs:range code:CodeUnit .

code:child a owl:ObjectProperty ;
  rdfs:domain code:CodeUnit ;
  rdfs:range code:CodeUnit .
